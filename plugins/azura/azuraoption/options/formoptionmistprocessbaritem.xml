<?xml version="1.0" encoding="UTF-8"?>
<form>
    <fields name="elementAttrs">
        <fieldset name="elementAttrsFieldSet">
            <field
                type="text"
                name="name"
                id="name"
                class="input-xlarge"
                label="Process Name"
                description="Process Name"  />
            <field
                type="text"
                name="percent"
                id="percent"
                class="input-xlarge"
                label="Percent"
                description="Percent"  />
        </fieldset>
    </fields>
    <fields name="elementSettings">
        <fieldset name="elementSettingsFieldSet">
            <field
                    type="AzuraElementSetting"
                    name="showStyleTab"
                    setting="false"
                    default="true"/>
            <field
                    type="AzuraElementSetting"
                    name="showAnimationTab"
                    setting="false"
                    default="true"/>
            <field
                    type="AzuraElementSetting"
                    name="numberLeftSettings"
                    setting="0"
                    default="all"/>
            <field
                    type="AzuraElementSetting"
                    name="contentFirst"
                    setting="false"
                    default="false"/>
        </fieldset>
    </fields>
</form>

